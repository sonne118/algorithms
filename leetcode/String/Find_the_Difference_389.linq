//Input: s = "abcd", t = "abcde"
//Output: "e"
//Explanation: 'e' is the letter that was added.


public char FindTheDifference1(string s, string t)
{
	//char result = (char);
	char result = ' ';
	foreach (var ch in s)
		result ^= ch;
	foreach (var ch in t)
		result ^= ch;

	return result;
}